<section id="contact">
  <div class="container">
    <div class="well well-sm">
      <h3><strong>Contact Us</strong></h3>
    </div>
	
	<div class="row">
	  <div class="col-md-7">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d54552.91760071065!2d30.037731220070434!3d31.288337154801976!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14f5d133b1b427c9%3A0x919bea8e9bae7710!2sArab+Academy+For+Science%2C+Technology+%26+Maritime+Transport!5e0!3m2!1sen!2seg!4v1532789493819" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
